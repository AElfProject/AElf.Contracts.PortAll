<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>AElf.Contracts.HelloWorldContract</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsContract>true</IsContract>
  </PropertyGroup>
  <ItemGroup>
    <ContractCode Include="..\..\protobuf\hello_world.proto">
      <Link>Protobuf/Proto/hello_world.proto</Link>
    </ContractCode>

    <ContractReference Include="..\..\protobuf\acs6.proto">
      <Link>Protobuf/Proto/acs6.proto</Link>
    </ContractReference>
  </ItemGroup>
</Project>